<section class="demo-block demo-slider">
        <div class="row">
            <div class="col-lg-12">
                <h2 class="header-block">{{header}}</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-8 col-md-offset-2 col-lg-12 col-lg-offset-0">
                <div class="owl-carousel image-carousel-{{id}}">
                    {{#if images.length}}
                        {{#each_with_sort images "order"}}
                            <div class="item item-image">
                                <img src="{{image}}" alt="">
                            </div>
                        {{/each_with_sort}}
                    {{/if}}
                </div>                
            </div>
        </div>
</section>

<script type="text/javascript">
jQuery(document).ready(function() {
    jQuery('.image-carousel-{{id}}').owlCarousel({
        autoPlay: 3000, //Set AutoPlay to 3 seconds
        margin: 10,
        items : 3,
        itemsDesktop : [1199,3],
        itemsDesktopSmall : [979,3]
  });
});
</script>