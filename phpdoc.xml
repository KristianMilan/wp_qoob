<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <parser>
        <target>docs/phpapi</target>
    </parser>
    <transformer>
        <target>docs/phpapi</target>
    </transformer>
    <files>
        <directory>includes/</directory>
        <directory>modules/</directory>

        <ignore>index.php</ignore>
        <ignore>dev/*</ignore>
        <ignore>tests/*</ignore>
        <ignore>node_modules/*</ignore>
        <ignore>aq_resizer.php</ignore>
        <ignore>build/*</ignore>
        <ignore>docs/*</ignore>
        <ignore>templates/*</ignore>
        <ignore>preview.php</ignore>
    </files>
    <transformations>
        <template name="dev/new_clean" />
    </transformations>
</phpdoc>